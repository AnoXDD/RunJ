<Window x:Class="RunJ.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RunJ"
        Height="125"
        Width="360"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Topmost="True"
        WindowStyle="None"
        Background="White"
        Opacity="1"
        AllowsTransparency="True"
        Closing="MainWindow_Closing"
        KeyUp="MainWindow_OnKeyUp" PreviewKeyDown="MainWindow_OnKeyDown"  IsTabStop="False">
    <Grid Background="White"
          Margin="0,0,0,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="179*"/>
        </Grid.ColumnDefinitions>
        <Label Name="FocusIndicator"
               Content="" Width="5" Height="5" Margin="343.069,10,10,110.333" Panel.ZIndex="1000" Background="Black"
               Padding="0" Grid.Column="1" />
        <Label x:Name="InfoTime"
               HorizontalAlignment="Center"
               Content="00:00 00"
               FontSize="75"
               Padding="10,0,0,0"
               Margin="33.069,0,35,20.333" Grid.Column="1" />
        <Canvas Margin="0,18,342,10.333" Grid.ColumnSpan="2">
            <Label x:Name="Arrow" Content=">" Width="7"  Height="15" Panel.ZIndex="1000" Opacity="0" Canvas.Left="11" FontSize="12" Padding="0" FontWeight="Bold" />
        </Canvas>
        <TextBox Height="29"
                 Name="Command"
                 TextWrapping="Wrap"
                 VerticalAlignment="Top"
                 Margin="0,4,0,0"
                 HorizontalAlignment="Left"
                 Width="353"
                 FontSize="15"
                 FontFamily="Consolas"
                 Background="White"
                 BorderThickness="0"
                 Padding="23,10,10,0"
                 TextChanged="Command_TextChanged"
                 GotKeyboardFocus="Command_GotKeyboardFocus"
                 LostKeyboardFocus="Command_LostKeyboardFocus"
                 Opacity="0" IsTabStop="False" Grid.ColumnSpan="2" />
        <TextBox Height="82"
                 TextWrapping="Wrap"
                 VerticalAlignment="Top"
                 Margin="21.069,33,-3,0"
                 HorizontalAlignment="Left"
                 Width="340"
                 FontSize="15"
                 FontFamily="Consolas"
                 Background="White"
                 BorderThickness="0"
                 Name="Predictions"
                 Padding="0,10,10,0"
                 Opacity="0" Panel.ZIndex="2" IsTabStop="False" Grid.Column="1" />
        <Label x:Name="InfoDate" Content="&#xD;&#xA;"
               HorizontalContentAlignment="Center"
               HorizontalAlignment="Center" Margin="0,91,0,0"
               VerticalAlignment="Top" Width="360" Height="24" Grid.ColumnSpan="2" />
        <Label x:Name="VersionLabel" Content="1.0.0" HorizontalContentAlignment="Right"
               VerticalContentAlignment="Bottom" HorizontalAlignment="Right" Height="26" Margin="0,0,0,0.333"
               VerticalAlignment="Bottom" Width="55" FontSize="5.333" Grid.Column="1" />

    </Grid>
</Window>